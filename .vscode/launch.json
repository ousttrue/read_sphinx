{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "parser.py",
            "type": "python",
            "python": "${workspaceFolder}/.venv/Scripts/python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/parser.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "debugStdLib": true
        },
        {
            "name": "print_env",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/print_env.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "dump_doctree",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/dump_doctree.py",
            "args": [
                "build/.doctrees/index.doctree"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Sphinx",
            "type": "python",
            "request": "launch",
            "module": "sphinx",
            "args": [
                "src",
                "build"
            ],
            "justMyCode": false
        }
    ]
}